<!-- Page Header -->
<!-- Set your background image for this header on the line below. -->
<header class="intro-header" style="<%= %Q( background-image: url(#{image_path('blogg/frontend/blog/post-bg.jpg')}) ) %>" >
    <div class="container">
        <div class="row">
            <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
                <div class="post-heading">
                    <h1><%= h1_title %></h1>
                    <h2 class="subheading"><%= h2_title %></h2>
                    <span class="post-meta"> <%= foot %></span>
                </div>
            </div>
        </div>
    </div>
</header>
